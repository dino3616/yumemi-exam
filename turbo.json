{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "generate": {
      "outputs": ["$api.*"]
    },
    "lint": {
      "dependsOn": ["^generate"]
    },
    "fmt": {
      "dependsOn": ["^generate"]
    },
    "style": {
      "dependsOn": ["^generate"]
    },
    "build": {
      "dependsOn": ["^generate", "^build"],
      "outputs": [".next/**", "dist/**"]
    },
    "test": {
      "dependsOn": ["^build"]
    }
  }
}
